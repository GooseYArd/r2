define(`R2_PKG', `libaio')dnl
define(`R2_VERSION', `0.3.109')dnl
define(`R2_DIR', `R2_PKG-R2_VERSION')dnl
define(`R2_DIST', `libaio_0.3.109.orig.tar.gz')dnl
define(`R2_URL', `ftp://ftp.de.debian.org/debian/pool/main/liba/libaio/R2_DIST')dnl

include(`r2.m4')
R2_DEFAULT_RULES

.libaio.config:
	touch $@

.libaio.install: .libaio.make
	cd $(libaio.dir) && \
	make prefix=install install && \
	touch $(CWD)/.libaio.install

